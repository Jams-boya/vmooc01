.pageul
{
	list-style: none;
	margin: 0;
	padding: 0;
	display: table;
	margin: auto;
	margin-top: 60px;
}
.pageul li
{
	float: left;
	width: 30px;
	height: 30px;
	text-align: center;
	line-height: 30px;
	border: 1px solid #DDDDDD;
	margin-left: 10px;
	border-radius: 2px;
	cursor: pointer;
}
.pageul li:first-child
{
	width: 40px;
	margin-left: 0;
}
.pageul li:last-child
{
	width: 40px;
}
.pageul li:hover
{
	background: #07A74F;
	color: white;
}