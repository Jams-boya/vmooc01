<div class="pubcon container" style="font-family: 'Microsoft YaHei';">
  <div class="load">
    <span><a href="/courseManage" style="color:black;font-family: 'Microsoft YaHei'">讲师端</a></span>&nbsp;>&nbsp;
    <span style="color:#00AA00;font-family: 'Microsoft YaHei'">课程管理</span>
  </div>

  <div class="pubmenu" style="float:left;width:13%;height:500px;">
    <!-- nav -->
  </div>
  <div class="con" style="margin-top:17px;">
    <div class="contitle">
      <span class="refer">课程管理</span>
      <div class="navigation" style="float:right;">
            <button class="btn btn-success" style="font-size:16px;color:white;position: absolute;margin-top: -20px;margin-left: -89px;" id="pCourse">发布课程</button>
            <div class="course-modal"></div>
        <div style="clear: none;"></div>
      </div>
      </div>
      <div style="clear: none;margin-bottom: 10px;"></div>
    </div>
    <div class="conbody" style=" width:85%;float:right;height:650px;border:1px solid #E0E1E1;">
      <div style="margin-top:5px;width:100%;height:50px;font-size:16px;text-align: center;">
        <div class="filter" style="float:left;margin-left:20px;height:100%;width:120px;">
          <div class="btn" id="all" style="width:100%;">全部课程</div>
          <div id="proc_bar" style="background: linear-gradient(to right, red , yellow , #27AE24); height: 3px;"></div>
        </div>
        <div class="filter" style="float:left;height:100%;width:120px;">
          <div class="btn" id="sell" style="width:100%;">售卖中的课程</div>
        </div>
        <div class="filter" style="float:left;height:100%;width:120px;">
          <div class="btn" id="process" style="width:100%;">建设中的课程</div>
        </div>
        <div class="filter" style="float:left;height:100%;width:120px;">
          <div class="btn" id="offShelve" style="width:100%;">违规下架</div>
        </div>
        <div class="filter" style="float:left;height:100%;width:120px;">
          <div class="btn" id="waitRevise" style="width:100%;">违规待审核</div>
        </div>
      </div>
      <div class="tableContainer"  style="height:680px;">
      </div>
    </div>

    </div>
  </div>
</div>
<script>
  var queryState;
  {{#if state}}
    queryState = Number('{{state}}') + 1;
  {{/if}}
</script>
{{#section 'appcss'}}
<!-- 新 Bootstrap 核心 CSS 文件 -->
<link href="http://cdn.static.runoob.com/libs/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
<link rel="stylesheet" href="/expert/expertPublish/bundle.css">
{{/section}}

{{#section 'appjs'}}
<!-- jQuery文件。务必在bootstrap.min.js 之前引入 -->
<script src="http://cdn.static.runoob.com/libs/jquery/2.1.1/jquery.min.js"></script>
<!-- 最新的 Bootstrap 核心 JavaScript 文件 -->
<script src="http://cdn.static.runoob.com/libs/bootstrap/3.3.7/js/bootstrap.min.js"></script>
<script src="/course/courseManage/bundle.js"></script>
<script>
  $(".load a").mouseover(function(e) {
    $(e.target).css("color","#00AA00");
  });
  $(".load a").mouseleave(function(e) {
    $(e.target).css("color","black");
  });
  
</script>
{{/section}}