<div class="pubcon container" style="font-family: 'Microsoft YaHei';">
  <div class="load">
    <span><a href="/myCourses" style="color:black">学员端</a></span>&nbsp;>&nbsp;
    <span style="color:#00AA00">我的课程</span>
  </div>

  <div class="pubmenu" style="float:left;width:13%;height:500px;">
    <!-- nav -->
  </div>
  <div class="con" style="margin-top:17px;">
    <div class="contitle">
      <span class="refer">我的课程</span>
      <div class="navigation">
        <div style="float:right;">
        <div style="clear: none;"></div>
      </div>
      </div>
      <div style="clear: none;margin-bottom: 10px;"></div>
    </div>
    <div class="conbody" style=" width:85%;float:right;height:700px;border:1px solid #E0E1E1;">
      <div style="margin-top:5px;width:100%;height:50px;font-size:16px;text-align: center;">
        <div class="filter" style="float:left;margin-left:20px;height:100%;width:120px;">
          <div class="btn" id="bought" style="width:100%;">我购买的</div>
          <div id="proc_bar" style="background: linear-gradient(to right, red , yellow , #27AE24); height: 3px;"></div>
        </div>
        <div class="filter" style="float:left;height:100%;width:120px;">
          <div class="btn" id="given" style="width:100%;">赠送给我的</div>
        </div>
      </div>
      <div class="tableContainer"  style="height:765px;">
      </div>
    </div>

    </div>
  </div>
</div>
<script>
  var isFrom;
  {{#if isFrom}}
    isFrom = {{isFrom}};
  {{/if}}

  $(".load a").mouseover(function(e) {
    $(e.target).css("color","#00AA00");
  });
  $(".load a").mouseleave(function(e) {
    $(e.target).css("color","black");
  });
</script>
{{#section 'appcss'}}
<!-- 新 Bootstrap 核心 CSS 文件 -->
<link href="http://cdn.static.runoob.com/libs/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
<link rel="stylesheet" href="/expert/expertPublish/bundle.css">
{{/section}}

{{#section 'appjs'}}
<!-- jQuery文件。务必在bootstrap.min.js 之前引入 -->
<script src="http://cdn.static.runoob.com/libs/jquery/2.1.1/jquery.min.js"></script>
<!-- 最新的 Bootstrap 核心 JavaScript 文件 -->
<script src="http://cdn.static.runoob.com/libs/bootstrap/3.3.7/js/bootstrap.min.js"></script>
<script src="/course/myCourse/bundle.js"></script>

{{/section}}