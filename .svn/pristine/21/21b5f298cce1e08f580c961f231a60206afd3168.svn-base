body
{
	margin: 0;
}
.clear
{
	clear: both;
}
.prodet
{
	font-family: "Microsoft YaHei";
	min-width: 1010px;
}
.prodet .prodetcon
{
	margin-top: 80px;
}
.prodet .prodetcon .proright
{
	width: 250px;
	float: right;
}
.prodet .prodetcon .proleft
{
	float: left;
	width: calc(100% - 300px);
	width: -moz-calc(100% - 300px);
	width: -webkit-calc(100% - 300px);
	width: -ms-calc(100% - 300px);
	min-width: 375px;
}
.prodet .prodetcon .proleft .promenu
{
	font-size: 16px;
	padding-bottom: 15px;
	border-bottom: 1px solid #DDDDDD;
}
.prodet .prodetcon .proleft .promenu span
{
	padding: 0 20px 0 20px;
	cursor: pointer;
	position: relative;
	-moz-user-select: none;
	-ms-user-select: none;
	-webkit-user-select: none;
}

.prodet .prodetcon .proleft .promenu span .menuline
{
	position: absolute;
	width: 100%;
	height: 3px;
	background: linear-gradient(to right, red , yellow , #27AE24);
	background: -moz-linear-gradient(to right, red , yellow , #27AE24);
	background: -ms-linear-gradient(to right, red , yellow , #27AE24);
	bottom: -18px;
	display: none;
	left: 0;
}
.prodet #procon1
{
	margin-top: 60px;
}
.prodet #procon2
{
	margin-top: 60px;
}
.prodet .prodetcon .proleft .procon
{
	padding-left: 20px;
	font-size: 14px;
	margin-top: 30px;
}

.prodet .prodetcon .proleft .procon .procontitle
{
	display: block;
	font-size: 15px;
}
.prodet .prodetcon .proleft .procon .proul
{
	list-style: none;
	color: #656565;
	margin: 0;
	padding: 0;
	margin-left: 20px;
	margin-top: 10px;
	line-height: 200%;
}
.prodet .protec
{
	position: relative;
	margin-top: -94px;
	z-index: 999;
}
.prodet .protec .tecimg
{
	width: 180px;
	height: 180px;
	border-radius: 50%;
	background: #7ACA5B;
	float: left;
	overflow: hidden;
}
.prodet .protec .tecimg img
{
	width: 100%;
}
.prodet .protec .teccon
{
	float: right;
	width: calc(100%  - 200px);
	width: -moz-calc(100%  - 200px);
	width: -webkit-calc(100%  - 200px);
	width: -ms-calc(100%  - 200px);
}
.prodet .protec .tec
{
	margin-top: 20px;
}
.prodet .protec .tec:first-child span:first-child
{
	font-size: 22px;
}
.prodet .protec .tec:first-child span:last-child a
{
	color: #27AE24;
	margin-left: 20px;
	text-decoration: none;
}
.prodet .protec .tec:nth-child(1)
{
	margin-top: 5px;
}
.prodet .protec .tec:nth-child(2)
{
	margin-top: 10px;
	font-size: 17px;
}
.prodet .protec .tec:nth-child(3)
{
	margin-top: 40px;
	color: #656565;
}
.prodet .porjtitle
{
	height: 40px;
	line-height: 40px;
	font-size: 16px;
}

.prodet .projcon
{
	margin-top: 40px;
}
.prodet .porjtitle span
{
	padding: 0 20px 0 20px;
	position: relative;
}
.prodet .porjtitle .markdiv
{
	width: 2px;
	background: #27AE24;
	height: 60%;
	position: absolute;
	top: 30%;
	left: 10px;
}
.prodet .projcontent
{
	height: 40px;
	line-height: 40px;
	font-size: 14px;
	color: #656565;
	border-bottom: 1px solid #E4E4E4;
}
.prodet .projcontent span
{
	padding: 0 20px 0 20px;
}
.prodet .projcontent span:nth-child(3)
{
	float: right;
}
.prodet .projcontent span:nth-child(4)
{
	float: right;
}
.prodet .projcontent span a
{
	text-decoration: none;
	color: #27AE24;
}
.prodet .projcontent span .playimg
{
	margin-top: 12px;
	width: 14px;
	float: left;
	cursor: pointer;
}
.prodet .projcontent span .playtime
{
	margin-left: 3px;
	padding: 0;
	color: #27AE24;
}
.prodet .quetop
{
	padding: 40px 0 40px 0;
	font-size: 16px;
	text-align: center;
	line-height: 200%;
}
.prodet .quetop a
{
	text-decoration: none;
	color: red;
	margin-left: 10px;
}
.prodet .quetitle
{
	padding-left: 20px;
	height: 40px;
	line-height: 40px;
	font-size: 14px;
}
.prodet .quelist
{
	padding: 20px 0 20px 20px;
	border-top: 1px solid #E4E4E4;
	font-size: 14px;
}
.prodet .quelist:first-child
{
	border-top: 0px;
}
.prodet .quelist a
{
	text-decoration: none;
}
.prodet .quelist span
{
	padding: 0 20px 0 20px;
	text-align: center;
	display: table;
	float: left;
}
.prodet .quelist .peekCount
{
	padding: 0 2px 0 2px;
	color: black;
	float: none;
	display: inline;
}
.prodet .quelist .quename
{
	padding: 0;
}
#see {
	color : #27AE24;
}
#agree {
	color : red;
}
.prodet .quelist .spansm
{
	font-size: 13px;
	color: #ababab;
}
.prodet .quelist .spansm a
{
	color: #ababab;
}
.prodet .quelist .question
{
	color: #656565;
}
.prodet .queload
{
	text-align: center;
}
.prodet .queload a
{
	text-decoration: none;
	color: black;
	font-size: 14px;
	position: relative;
}
.prodet .queload a .jt
{
	position: absolute;
	width: 0;
	height: 0;
	border-top: 6px solid black;
	border-left: 6px solid transparent;
	border-right: 6px solid transparent;
	border-bottom: 6px solid transparent;
	right: -16px;
	top: 8px;
}
.prodet .promenu .re
{
	position: absolute;
	width: 20px;
	height: 20px;
	line-height: 20px;
	text-align: center;
	font-size: 12px;
	color: white;
	background: red;
	border-radius: 3px;
	right: 0px;
	top: -18px;
}
.prodet .triangle
{
	width: 0px;
	height: 0px;
	border-top: 8px solid black;
	border-left: 8px solid transparent;
	border-right: 8px solid transparent;
	border-bottom: 8px solid transparent;
	margin: auto;
	cursor: pointer;
}
.prodet .probanner
{
	width: 100%;
	height: 400px;
	overflow: hidden;
}
.prodet .probanner img
{
	width: 100%;
}
.project {
	padding-bottom: 40px;
}
#answerCount {
	padding: 0;
}




.vlistdiv
{
	position: relative;
	min-width: 640px;
	margin-bottom: 40px;
}
.vlistdiv .vlist
{
	width: 200px;
	margin-top: 30px;
	margin-left: calc((100% - 600px) / 2);
	margin-left: -moz-calc((100% - 600px) / 2);
	margin-left: -webkit-calc((100% - 600px) / 2);
	margin-left: -ms-calc((100% - 600px) / 2);
	float: left;
}
.vlistdiv .vlist .vlistimg
{
	height: 120px;
	background: #9D9F9A;
	line-height: 120px;
	text-align: center;
	overflow: hidden;
	display: block;
}
.vlistdiv .vlist .vlistimg img
{
	width: calc(100% + 5px);
	width: -webkit-calc(100% + 5px);
	width: -ms-calc(100% + 5px);
	width: -moz-calc(100% + 5px);
	width: -o-calc(100% + 5px);
	margin-top: -1px;
	margin-left: -2px;
}
.vlistcon
{
	height: 40px;
	overflow: hidden;
	margin-top: 5px;
}
.vlistcon a
{
	text-decoration: none;
	color: #686868;
	font-size: 13px;
}
.vlistfoot
{
	margin-top: 5px;
	font-size: 13px;
	height: 20px;
}
.vlistfoot span:first-child
{
	color: red;
	float: left;
}
.vlistfoot span:last-child
{
	color: #919191;
	float: right;
}