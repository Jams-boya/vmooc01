<div class="modal fade" id="recordList" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" >
        <div class="modal-dialog">
            <div class="modal-content" style="width:800px;">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title" style="float:left;" id="myModalLabel">赠送记录</h4>
                </div>
                <div class="modal-body recordListBody" style="height:300px;overflow-y:auto;">

                </div>
                <div class="modal-footer" style="padding-top:40px;">
                  <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
                </div>
            </div>
        </div>
</div>
