body {
	font-family: "Microsoft Yahei"!important;
}
.conbody {
	border: 1px solid #E4E4E4;
}
.refer{
	font-size: 15px;
	float: left;
	margin-left: 20px;
}
.navigation{
	float: right;
}
.basicInfoC .message{
	width: 100%;
}
.basicInfoC .message .messdiv{
	border-top: 0px;
	border-left: 0px;
	border-right: 0px;
	border-bottom: 1px solid #E4E4E4;
	padding: 10px 0 10px 0;
	color: #797979;
	border-bottom: 0;
}
.basicInfoC .message .messdiv:last-child{
	border-top: 0px;
	border-left: 0px;
	border-right: 0px;
	border-bottom: 1px solid #E4E4E4;
}
.basicInfoC .message .messhead{
	color: black;
}
.basicInfoC .message .messhead .messimg{
	height: 20px;
}
.basicInfoC .message .messhead .messcon{
	text-align: center;
}
.basicInfoC .message .messhead .messhour{
	height: 20px;
}
.basicInfoC table{
	width:100%;
}
.basicInfoC .addtoc{
	margin-left: 20px;
	margin-bottom: 20px;
	color: #80B9DF;
}
.basicInfoC .addtoc button{
	border:0px;
	background: none;
}
.basicInfoC .addtoc button:focus {
	outline-style: none;
}
.basicInfoC .addclazz{
	margin-left: 60px;
	margin-top: 10px;
}
.basicInfoC .addclazz button{
	width: 12%;
	height: 32px;
	border: 1px solid #80D5AF;
	color: #80D5AF;
	background: none;
}
.basicInfoC .addclazz button:focus {
	outline-style: none;
}
.basicInfoC .upclazz{
	border: 0px;
	background: none;
	font-size: 14px;
	font-family: "Microsoft Yahei"!important;
}
.basicInfoC .upclazz:focus {
	outline-style: none;
}
.basicInfoC .backclazz{
	border: 0px;
	background: none;
	font-size: 14px;
	font-family: "Microsoft Yahei"!important;
}
.basicInfoC .backclazz:focus {
	outline-style: none;
}
.basicInfoC .tocidx{
	font-size:15px;
	font-family: "Microsoft Yahei"!important;
	margin-right: 5px;
}
.basicInfoC .tocname input{
	font-size: 14px;
	font-family: "Microsoft Yahei"!important;
	border:1px solid #F7F8F9;
	padding: 4px;
}
.basicInfoC .tocname input:focus {
	outline-style: none;
}
.basicInfoC .clazzname input{
	font-size: 14px;
	font-family: "Microsoft Yahei"!important;
	border:1px solid #F7F8F9;
	padding: 4px;
}
.basicInfoC .clazzname input:focus {
	outline-style: none;
}
.basicInfoC .barclazz{
	margin-top: 3px;
}
.basicInfoC tr td:nth-child(1) {
	width: 55%;
	font-size:14px;
}
.basicInfoC tr td:nth-child(2) {
	width: 5%;
	font-size:14px;
}
.basicInfoC tr td:nth-child(3) {
	width: 11%;
	font-size:14px;
}
.basicInfoC tr td:nth-child(4) {
	width: 20%;
	font-size:14px;
}
.basicInfoC tr td{
	padding: 0 10px 0 10px;
}
.basicInfoC .clazz{
	margin-left:4%;
	font-size: 14px;
	font-family: "Microsoft Yahei"!important;
}
.basicInfoC .bar td:nth-child(1) {
	width: 55%;
	font-size:14px;
}
.basicInfoC .bar td:nth-child(2) {
	width: 10%;
	font-size:14px;
}
.basicInfoC .bar td:nth-child(3) {
	width: 15%;
	font-size:14px;
}
.basicInfoC .bar td:nth-child(4) {
	width: 20%;
	font-size:14px;
}
.basicInfoC .bar td{
	padding: 0 10px 0 10px;
}
.basicInfoC .updown{
	/*width:20px;*/
	height:20px;
	cursor:pointer;
}
.basicInfoC input.free{
	margin: 0px 3px 0px 10px;
	font-family: "Microsoft Yahei"!important;
	font-size: 10px;
}
.basicInfoC .delete{
	background: red;
}
.basicInfoC .footer{
	margin-top: 40px;
	margin-bottom: 40px;
	text-align: center;
}
.basicInfoC .load span a{
	text-decoration:none;
	color:black;
}
.basicInfoC .load span a:hover{
	color:red;
}
/*左侧菜单栏修改*/
.pubmenu #accordion{
	box-shadow: 0px 0px 0px 1px #E0E1E1;
	font-family: "Microsoft Yahei"!important;
	margin-top: 40px;
}
/*test*/
.navli{
	margin-left: 20px;
}
.navdisc{
	height: 10px;
	float: right;
}
.navdisc span{
	display: table;
	float: left;
	font-family: "Microsoft Yahei"!important;
	font-size: 14px;
}
.disc{
	width: 8px;
	height: 8px;
	border-radius: 50%;
	background: #DDDEDF;
	float: left;
}
.line{
	height: 1px;
	width: 40px;
	background: #DDDEDF;
	float: left;
	margin-left: 10px;
	margin-right: 10px;
	margin-top: 3.5px;
}
.basicInfoC .tabtop td{
	font-family: "Microsoft Yahei"!important;
	font-size: 14px;
}
.basic {
	display: none;
}
#toctop{
	border-bottom: 1px solid #E0E1E1;
}
.publish:focus {
	outline-style: none;
}
.draft:focus {
	outline-style: none;
}
.tips {
	margin-left: 100px;
}
.load span a {
	color: #000000;
}
.load span a:hover {
	color: #21BA10;
}
.load{
	font-size:12px;
}
