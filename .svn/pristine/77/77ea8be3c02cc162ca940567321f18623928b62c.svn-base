body {
	font-family: "Microsoft Yahei"!important;
}
.classifymanage{
	/*width:60%;*/
	border:1px solid #E4E5E6;
}
.classifymanage #tableTop{
	width: 100%;
	color:white;
	background: #37D89A;
}
.classifymanage #tableTop td:nth-child(1) {
	width:50%;
	padding:3px;
	text-align: center;
}
.classifymanage #tableTop td:nth-child(2) {
	width:10%;
	text-align: center;
}
.classifymanage #tableTop td:nth-child(3) {
	width:40%;
	text-align: center;
}
.classifymanage .title{
	width: 100%;
}
.classifymanage .titlebox{
	padding:5px;
}
.classifymanage .detail{
	width: 100%;
	border-collapse: collapse;
}
.classifymanage .detail tr{
	border-top: 1px solid #E4E5E6;
	border-bottom: 1px solid #E4E5E6;
}
.classifymanage .detail td:nth-child(1) {
	width:50%;
	margin-left: 100px;
	padding: 5px;
}
.classifymanage .addInput{
	margin-left: 20%;
  font-size: 14px;
  padding: 3px;
}
.classifymanage .addInput:focus{
	outline-style: none;
}
.classifymanage .detail td:nth-child(2) {
	width:10%;
	text-align: center;
}
.classifymanage .detail td:nth-child(3) {
	width: 40%;
	text-align: center;
}
.classifymanage .detail .up{
  width:15px;
  height:15px;
	cursor: pointer;
}
.classifymanage .detail .down{
  width:15px;
  height:15px;
	cursor: pointer;
}
.classifymanage .delete{
	color: #43C95D;
	cursor: pointer;
}
.classifymanage .addclassify{
	margin-left: 60px;
	margin-top: 10px;
	padding:3px;
}
.classifymanage .addclassify button{
	width: 8%;
	height: 32px;
	border: 1px solid #43C95D;
	color: white;
	background: #43C95D;
	cursor: pointer;
}
.classifymanage .addclassify button:focus{
	outline-style: none;
}
.classifymanage .save{
  width: 120px;
  height: 30px;
  line-height: 30px;
  color: white;
  background: #3CCC86;
	border:0px;
  text-align: center;
  cursor: pointer;
  border-radius: 4px;
  margin:0px auto;
  user-select: none;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  -o-user-select: none;
}