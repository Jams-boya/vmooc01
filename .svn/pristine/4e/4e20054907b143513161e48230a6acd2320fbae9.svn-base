                  <span ng-bind="account.aliyunId" title="account.aliyunIdOrigin" class="ng-binding"></span>
                  <span class="icon-arrow-down"></span>
                </a>
                <ul class="dropdown-menu">
                  <!-- ngIf: account.currentStructure -->
                  <!-- ngIf: account.loginStructure -->
                  <!-- ngRepeat: link in navLinks.user.links -->
                  <li ng-repeat="link in navLinks.user.links" class="topbar-info-btn ng-scope">
                    <a href="https://account.aliyun.com/logout/logout.htm?oauth_callback=https%3A%2F%2Fecs.console.aliyun.com%2F%23%2Fserver%2Fregion%2Fcn-hangzhou" role="menuitem" target="_self" aliyun-console-spm="600" data-spm-click="gostr=/aliyun;locaid=d600;;">
                      <span class="ng-binding">退出</span>
                    </a>
                  </li>
                  <!-- end ngRepeat: link in navLinks.user.links -->
                  <!-- ngIf: account.currentStructure --> </ul>
              </div>
            </div>
            <!-- end ngIf: navLinks.user.show -->
            <!-- 国际化 -->
            <!-- ngIf: navLinks.i18n.show && navLinks.i18n.currentLanguage --> </div>
          <!-- ngIf: navLinks.customHelp.show && helpConfig --> </div>
