            </a>
            <a href="http://home.console.aliyun.com" role="menuitem" aria-haspopup="false" target="_self" class="topbar-home-link topbar-btn topbar-left" aliyun-console-spm="2" data-spm-click="gostr=/aliyun;locaid=d2;;">
              <span ng-bind="navLinks.home.text" class="ng-binding">{{user.companyName}}</span>
            </a>
          </div>
          <div class="topbar-info topbar-right topbar-clearfix">
           
            <div class="topbar-left topbar-workorder topbar-info-item ng-scope" ng-if="navLinks.workorder.show">
              <div class="dropdown" dropdown="">
                <a href="#" role="menuitem" aria-haspopup="true" aria-label="press enter to expand" class="dropdown-toggle topbar-btn" data-toggle="dropdown" aliyun-console-spm="51" data-spm-click="gostr=/aliyun;locaid=d51;;">
                  <span ng-bind="navLinks.workorder.text" class="ng-binding">工单服务</span>
                  <!-- ngIf: navLinks.workorder.showNew -->
                  <span class="icon-arrow-down"></span>
                </a>
                <ul class="dropdown-menu">
                  <!-- ngRepeat: link in navLinks.workorder.links -->
                  <li ng-repeat="link in navLinks.workorder.links" class="topbar-info-btn ng-scope">
                    <a href="https://workorder.console.aliyun.com/#/ticket/list/" role="menuitem" target="_blank" aliyun-console-spm="510" data-spm-click="gostr=/aliyun;locaid=d510;;">
                      <span class="ng-binding">我的工单</span>
                      <!-- ngIf: link.showNew --> </a>
                  </li>
                  <!-- end ngRepeat: link in navLinks.workorder.links -->
                  <li ng-repeat="link in navLinks.workorder.links" class="topbar-info-btn ng-scope">
                    <a href="https://home.console.aliyun.com/common/navRedirect.html?typeId=workorderAdd&amp;productId=ecs" role="menuitem" target="_blank" aliyun-console-spm="511" data-spm-click="gostr=/aliyun;locaid=d511;;">
                      <span class="ng-binding">提交工单</span>
                      <!-- ngIf: link.showNew --> </a>
                  </li>
                  <!-- end ngRepeat: link in navLinks.workorder.links --> </ul>
              </div>
            </div>
            <!-- end ngIf: navLinks.workorder.show -->
