<div class="pubcon container" style="font-family: 'Microsoft YaHei';">
  <div class="load">
    <span><a href="/personal?url=/personal">讲师端</a></span>&nbsp;>&nbsp;
    <span style="color:#00AA00">问答管理</span>
  </div>

  <div class="pubmenu" style="float:left;width:13%;height:500px;">
    <!-- nav -->
  </div>
  <div class="con" style="margin-top:17px;">
    <div class="contitle">
      <span class="refer">问答管理</span>
      </div>
      <div style="clear: none;margin-bottom: 10px;"></div>
    </div>
    <div class="conbody" style=" width:85%;float:right;border:1px solid #E0E1E1;height: 720px;">
      <div class="awm">
        <div class="expertId">{{expertId}}</div>
        <div class="awmMenu">
          <span>
            全部问答
            <div class="spanLine"></div>
          </span>
          <span>
            待回答
            <div class="spanLine"></div>
          </span>
          <div class="clear"></div>
        </div>
        <div class="awmcon"></div>
      </div>
    </div>
      <!-- body -->
    </div>
  </div>
  <!--<div style="clear: both;"></div>-->
</div>
<script>
  var queryState;
  {{#if state}}
    queryState = '{{state}}';
  {{/if}}
</script>
{{#section 'appcss'}}
<link rel="stylesheet" href="/expert/expertPublish/bundle.css">
<link rel="stylesheet" href="/course/answerManage/bundle.css">
{{/section}}

{{#section 'appjs'}}
<script src="/course/answerManage/bundle.js"></script>
<script>
  $(".load a").mouseover(function(e) {
    $(e.target).css("color","#00AA00");
  });
  $(".load a").mouseleave(function(e) {
    $(e.target).css("color","black");
  });
</script>
{{/section}}