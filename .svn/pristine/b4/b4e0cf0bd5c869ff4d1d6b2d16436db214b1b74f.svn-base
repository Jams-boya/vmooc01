@media (min-width: 992px) {
  .container {
     width: auto; 
     max-width: 1000px;
     margin: auto;
  }
}
.common .head div{
  max-width: 1000px;
}
.signModal {
  background: -webkit-linear-gradient(#64C2F6, #BBE5FC); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(#64C2F6, #BBE5FC); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(#64C2F6, #BBE5FC); /* Firefox 3.6 - 15 */
  background: linear-gradient(#64C2F6, #BBE5FC); /* 标准的语法（必须放在最后） */
  padding-top: 0px;
}

.signContent {
  background-image: url(/images/jjfa.png);
  background-size:50% 80%;
  background-repeat:no-repeat;
  margin-left: 4%;
  margin-top: 10px;
  /*min-height: 600px;*/
}

#formSignin {
  width: 320px;
  margin-left:60%;
  min-height:320px;
  margin-top: 6%;
  background-color: white;
  padding-bottom: 40px;
}

.input-icon {
  display: block;
  float: left;
  font-size: 18px;
  width: 38px
}

.signupContent {
  margin-top: 20px;
  min-height: 600px;
  /*min-width: 1000px;*/
  max-width: 1088px;
  margin: 20px auto;
  position: relative;
}
/*body {
  font-family: "Microsoft YaHei";
}*/

.signModal .bg {
  position: relative;
  background-repeat:round;
  margin-top: 20px;
  width: calc(100% - 400px);
  width: -webkit-calc(100% - 400px);
  width: -moz-calc(100% - 400px);
  width: -ms-calc(100% - 400px);
  width: -o-calc(100% - 400px);
  height: 500px;
  float: left;
  overflow: hidden;
}
.signModal .bg img {
  position: absolute;
  right: 0px;
  top: 0px;
  height: 100%;
}
.company_signup_content {
  border-top: 1px dashed #AFAFAF; 
  margin-top: 20px;
  padding-top: 20px;
